  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:generate": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "db:migration:reset": "prisma migrate reset --schema=./src/database/prisma/schema.prisma",
    "db:migration:run": "prisma migrate deploy --schema=./src/database/prisma/schema.prisma",
    "db:migration:dev:run": "prisma migrate dev --preview-feature --schema=./src/database/prisma/schema.prisma",
    "lint": "eslint \"{src,apps,libs,e2e}/**/*.ts\" --fix",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:cov": "jest --config jest.config.ts --coverage",
    "test:e2e": "jest --config e2e-jest.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
